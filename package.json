{
  "name": "mbse-benchmark",
  "version": "0.1.0",
  "description": "Benchmarking framework for AI models on MBSE tasks",
  "author": "Gabriel Nepomuceno",
  "license": "MIT",
  "type": "module",
  "bin": {
    "mbse-bench": "./dist/cli.js"
  },
  "scripts": {
    "bench": "bun run ./src/cli/index.tsx",
    "build": "bun run build:current",
    "build:current": "bun build ./src/cli/index.tsx --compile --outfile ./dist/mbse-bench",
    "build:all": "bun run build:linux-x64 && bun run build:linux-arm64 && bun run build:darwin-x64 && bun run build:darwin-arm64 && bun run build:windows-x64",
    "build:linux-x64": "bun build ./src/cli/index.tsx --compile --target=bun-linux-x64 --outfile ./dist/mbse-bench-linux-x64",
    "build:linux-arm64": "bun build ./src/cli/index.tsx --compile --target=bun-linux-arm64 --outfile ./dist/mbse-bench-linux-arm64",
    "build:darwin-x64": "bun build ./src/cli/index.tsx --compile --target=bun-darwin-x64 --outfile ./dist/mbse-bench-darwin-x64",
    "build:darwin-arm64": "bun build ./src/cli/index.tsx --compile --target=bun-darwin-arm64 --outfile ./dist/mbse-bench-darwin-arm64",
    "build:windows-x64": "bun build ./src/cli/index.tsx --compile --target=bun-windows-x64 --outfile ./dist/mbse-bench-windows-x64.exe",
    "build:js": "bun build ./src/cli/index.tsx --outfile ./dist/cli.js --target node",
    "test": "bun test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \"#node_modules\" \"#site\" \"#.plan\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "site:dev": "cd site && bun run dev",
    "site:build": "cd site && bun run build",
    "prepublishOnly": "bun run build:js"
  },
  "files": [
    "dist",
    "config",
    "data/tasks"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "markdownlint-cli2": "^0.20.0",
    "prettier": "^3.7.4",
    "react-devtools-core": "^7.0.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/azure": "^3.0.0",
    "@ai-sdk/openai": "^3.0.0",
    "ai": "^6.0.0",
    "ink": "^6.5.1",
    "react": "^19.2.3",
    "zod": "^4.2.1"
  }
}
